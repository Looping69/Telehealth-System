{
  "name": "telehealth-system",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "backend:dev": "cd server && npm run dev",
    "backend:build": "cd server && npm run build",
    "backend:start": "cd server && npm start",
    "start": "node scripts/start-with-backend.js",
    "medplum:start": "docker-compose -f docker-compose.simple.yml up -d",
    "medplum:stop": "docker-compose -f docker-compose.simple.yml down",
    "medplum:restart": "npm run medplum:stop && npm run medplum:start",
    "medplum:logs": "docker-compose -f docker-compose.simple.yml logs -f",
    "medplum:reset": "docker-compose -f docker-compose.simple.yml down -v && docker system prune -f",
    "medplum:status": "docker-compose -f docker-compose.simple.yml ps"
  },
  "keywords": [],
